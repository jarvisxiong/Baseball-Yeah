<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<!--指定特定数据库的jdbc驱动jar包的位置 -->
	<classPathEntry location="C:\Users\Administrator\.IntelliJIdea2016.1\config\jdbc-drivers\mysql-connector-java-5.1.35-bin.jar"/>

	<context id="ipoTables" targetRuntime="MyBatis3">
		<!--去除注释  -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://120.55.245.175:3306/xykd_dev1?autoReconnect=true" userId="xykd_dev1" password="DEV_104ewq"></jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

         <!-- 生存Model位置 -->
		<javaModelGenerator targetPackage="com.zhiduan.axp.dao.manager.bean" targetProject="src/main/generator">
			<!-- 是否对model添加 构造函数 -->
			<property name="constructorBased" value="true" />
			<!-- 是否允许子包，即targetPackage.schemaName.tableName -->
			<property name="enableSubPackages" value="false" />
			<!-- 从数据库返回的值被清理前后的空格  -->
			<property name="trimStrings" value="true" />
			
			<!-- 建立的Model对象是否 不可改变  即生成的Model对象不会有 setter方法，只有构造方法 -->
            <!-- <property name="immutable" value="true"/> -->
		</javaModelGenerator>

		<!-- 生成映射文件的位置 -->
		<sqlMapGenerator targetPackage="mappings" targetProject="src/main/generator">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>

        <!-- 生成Dao 的位置 -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.zhiduan.axp.dao.manager.dao" targetProject="src/main/generator">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>
	 
	    <!-- 表和类名映射 -->
		<table schema="xykd_dev1" tableName="tb_task_sub"  enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
			<property name="useActualColumnNames" value="false" />
		</table>
		
		
	</context>
</generatorConfiguration>


